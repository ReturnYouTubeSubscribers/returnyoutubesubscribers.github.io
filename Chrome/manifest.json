{
    "name": "Return YouTube Subscribers",
    "description": "Fixes YouTube's Subscriber Counts On Channel/Video Pages.",
    "manifest_version": 3,
    "version": "3.0",
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "js": [
                "main.js"
            ],
            "matches": [
                "https://*.youtube.com/c/*",
                "https://*.youtube.com/channel/*",
                "https://*.youtube.com/user/*",
                "https://*.youtube.com/watch?v=*",
                "https://*.youtube.com/@*",
                "https://*.youtube.com/*"
            ]
        }
    ],
    "host_permissions": [
        "https://api.mgcounts.com/*"
    ]
}